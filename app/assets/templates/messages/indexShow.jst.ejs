<ul>
  <% messages.each(function (message){ %>
    <li id=<%= message.id %>><%= message.escape("title") %>
      <button class="copy_link" data-clipboard-text="<%= window.location.origin%>/#/messages/<%=message.id%>">Copy Link</button>
      <button class="edit">Edit</button>
      <button class="delete">Delete</button>
    </li>
    <br>
    <% }); %>
  </ul>