<li class="panel panel-primary msg-li" id="<%= message.id %>">
  <div class="panel-heading">
    <h3 class="panel-title msg-li-title"><%= message.get("title")%></h3>
  </div>
  <div class='panel-body msg-li-body'>
    <div class='msg-li-content'><%=message.escape("content")%></div>
    <div class='msg-li-timer'></div>
  </div>
</li>
<% var counter = message.get("timer"); %>
<% var interval = setInterval(function(){ %>
  <% if (counter == 0) { %>
    <% $('.msg-li').remove(); %>
    <% clearInterval(interval); %>
  <% } %>
  <% $('.msg-li-timer').html(counter)%>
  <%  counter--; %>
<%  }, 1000)%>